@page "/getelement"
@inject IJSRuntime JSRuntime

<h3 id="myElement">Hello from Blazor!</h3>

<button @onclick="GetBtn">Get Element by ID</button>

<p>@elementHtml</p>

@code {
    private string elementHtml;

    private async Task GetBtn()
    {
        elementHtml = await JSRuntime.InvokeAsync<string>("getElementById", "myElement");
    }
}